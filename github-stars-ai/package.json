{
  "name": "github-stars-ai",
  "version": "0.1.0",
  "description": "AI-powered GitHub stars analyzer and code generator using hierarchical LLM agents",
  "type": "module",
  "private": true,
  "engines": {
    "bun": ">=1.0.0"
  },
  "exports": {
    ".": {
      "import": "./dist/index.ts",
      "types": "./dist/types.ts"
    },
    "./config": {
      "import": "./packages/config/src/index.ts",
      "types": "./packages/config/src/types.ts"
    },
    "./agents": {
      "import": "./packages/agents/src/index.ts",
      "types": "./packages/agents/src/types.ts"
    },
    "./utils": {
      "import": "./packages/utils/src/index.ts",
      "types": "./packages/utils/src/types.ts"
    },
    "./workflow": {
      "import": "./packages/workflow/src/index.ts",
      "types": "./packages/workflow/src/types.ts"
    }
  },
  "imports": {
    "#config/*": "./packages/config/src/*",
    "#agents/*": "./packages/agents/src/*",
    "#utils/*": "./packages/utils/src/*",
    "#workflow/*": "./packages/workflow/src/*"
  },
  "workspaces": [
    "packages/*"
  ],
  "bin": {
    "github-stars-ai": "./src/index.ts"
  },
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "build": "bun run build:clean && bun run build:packages && bun run build:main",
    "build:clean": "rm -rf dist packages/*/dist",
    "build:packages": "bun run --parallel --cwd packages/* build",
    "build:main": "bun build ./src/index.ts --outdir ./dist --target node",
    "build:binary": "bun build ./src/index.ts --compile --outfile github-stars-ai",
    "lint": "biome check .",
    "lint:fix": "biome check --apply .",
    "format": "biome format .",
    "format:fix": "biome format --write .",
    "test": "bun test"
  },
  "dependencies": {
    "@langchain/anthropic": "latest",
    "@langchain/community": "^0.3.31",
    "@langchain/core": "^0.3.40",
    "@langchain/langgraph": "latest",
    "c12": "latest",
    "chalk": "latest",
    "citty": "latest",
    "consola": "latest",
    "destr": "latest",
    "hookable": "latest",
    "inquirer": "latest",
    "langchain": "^0.3.19",
    "nanospinner": "latest",
    "octokit": "latest",
    "rc9": "latest",
    "typesense": "latest",
    "unctx": "latest",
    "unplugin": "latest",
    "zod": "latest"
  },
  "devDependencies": {
    "@biomejs/biome": "latest",
    "@types/node": "latest",
    "bun-types": "latest",
    "husky": "latest",
    "typescript": "latest"
  },
  "module": "src/index.ts",
  "types": "dist/types.ts",
  "files": [
    "dist",
    "packages/*/dist"
  ],
  "keywords": [
    "ai",
    "github",
    "langchain",
    "claude",
    "anthropic",
    "code-analysis",
    "code-generation"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/github-stars-ai.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/github-stars-ai/issues"
  },
  "homepage": "https://github.com/yourusername/github-stars-ai#readme"
}
